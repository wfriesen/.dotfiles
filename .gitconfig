	email = wfriesen@gmail.com
	name = William Friesen
[diff]
	tool = diffmerge
[difftool "diffmerge"]
  cmd = ~/scripts/diffmerge-wrapper.sh $LOCAL $REMOTE
[color "grep"]
  linenumber = green
  match = bold red
  filename = magenta
[log]
  date = iso-local
[alias]
  diffone = "!git difftool \"$1\"^..\"$1\""
